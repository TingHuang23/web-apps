<h1> Contact List </h1>

<%
contacts = Contact.all
%>


<% #contacts.inspect%>

<ul>
  <% for contact in contacts %>
    <li>
      <%=contact["first_name"] %>
      <%=contact["last_name"] %>,
      <% company = Company.find_by({"id" => contact["company_id"]})%>
      <%=company["name"]%>  
    </li>
  <% end %>
  
</ul>